{
  "name": "eslint-plugin-rxjs-x",
  "type": "commonjs",
  "version": "0.0.0",
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "description": "ESLint v9+ plugin for RxJS",
  "author": "Jason Weinzierl <weinzierljason@gmail.com>",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/JasonWeinzierl/eslint-plugin-rxjs-x",
  "repository": {
    "type": "git",
    "url": "https://github.com/JasonWeinzierl/eslint-plugin-rxjs-x.git"
  },
  "bugs": {
    "url": "https://github.com/JasonWeinzierl/eslint-plugin-rxjs-x/issues"
  },
  "keywords": [
    "lint",
    "rules",
    "eslint",
    "rxjs"
  ],
  "main": "./dist/index.js",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dist": "yarn run lint && yarn run dist:build",
    "dist:build": "yarn run dist:clean && tsc -p tsconfig-dist.json",
    "dist:clean": "rimraf dist",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "prepublishOnly": "yarn run test && yarn run dist",
    "test": "yarn run test:rules && yarn run test:specs",
    "test:debug": "mocha -r ts-node/register -t 5000 tests/rules/no-redundant-notify.ts",
    "test:rules": "mocha -r ts-node/register -t 5000 tests/rules/*.ts",
    "test:specs": "mocha -r ts-node/register tests/*-spec.ts"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^5.0.0",
    "common-tags": "^1.8.0",
    "decamelize": "^5.0.0",
    "eslint-etc": "^5.1.0",
    "requireindex": "~1.2.0",
    "tslib": "^2.0.0",
    "tsutils": "^3.0.0",
    "tsutils-etc": "^1.4.1"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "typescript": ">=4.0.0"
  },
  "devDependencies": {
    "@cartant/eslint-config": "^3.0.0",
    "@types/chai": "^4.2.0",
    "@types/common-tags": "^1.8.0",
    "@types/eslint": "^8.0.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chai": "^4.2.0",
    "eslint": "^8.0.0",
    "lint-staged": "^13.0.0",
    "mocha": "^9.0.0",
    "rimraf": "^3.0.0",
    "rxjs": "^7.0.0",
    "ts-node": "^10.0.0",
    "typescript": "~4.7.4"
  }
}
